<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.cefetmg.view.SecondaryController" spacing="10" alignment="CENTER">
    <Label fx:id="labelUsuario" text="Usuário:"/>
    <TextField fx:id="TextUsuario" promptText="Digite seu usuário"/>
    
    <Label fx:id="labelSenha" text="Senha:"/>
    <TextField fx:id="TextSenha" promptText="Digite sua senha" text=""/>
    
    <Button fx:id="butaoEnviar" text="Enviar" onAction="#handleEnviar"/>
</VBox>
