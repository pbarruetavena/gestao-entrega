<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.PasswordField?>

<?import java.net.URL?>



 
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.cefetmg.view.Login" spacing="10" alignment="CENTER">
    <stylesheets>
        <URL value="@/styles/cadastrarproduto.css" />
    </stylesheets>
    <Label fx:id="labelUsuario" text="Usuário:"/>
    <TextField fx:id="TextUsuario" promptText="Digite seu usuário"/>
    
    <Label fx:id="labelSenha" text="Senha:"/>
    <PasswordField fx:id="SenhaText"  promptText="Digite sua senha"/>
    
    <Button fx:id="botao" text="Enviar" onAction="#handleEnviar"/>
</VBox>
